<?xml version="1.0" encoding="UTF-8"?>
  <web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0"
           metadata-complete="true">
  <display-name>Server</display-name>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.app.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NoticeServlet</servlet-name>
    <servlet-class>com.app.servlet.NoticeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CaloriesServlet</servlet-name>
    <servlet-class>com.app.servlet.CaloriesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CoachFitnessServlet</servlet-name>
    <servlet-class>com.app.servlet.CoachFitnessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DependServlet</servlet-name>
    <servlet-class>com.app.servlet.DependServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RelyServlet</servlet-name>
    <servlet-class>com.app.servlet.RelyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CourseServlet</servlet-name>
    <servlet-class>com.app.servlet.CourseServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DailyCheckServlet</servlet-name>
    <servlet-class>com.app.servlet.DailyCheckServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CommentServet</servlet-name>
    <servlet-class>com.app.servlet.CommentServet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FavorServlet</servlet-name>
    <servlet-class>com.app.servlet.FavorServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NewsServlet</servlet-name>
    <servlet-class>com.app.servlet.NewsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadServlet</servlet-name>
    <servlet-class>com.app.servlet.DownloadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TrainServlet</servlet-name>
    <servlet-class>com.app.servlet.TrainServlet</servlet-class>
  </servlet>







  <servlet-mapping>
  <servlet-name>UserServlet</servlet-name>
  <url-pattern>/User</url-pattern>
</servlet-mapping>
  <servlet-mapping>
    <servlet-name>NoticeServlet</servlet-name>
    <url-pattern>/Notice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CaloriesServlet</servlet-name>
    <url-pattern>/Calories</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CoachFitnessServlet</servlet-name>
    <url-pattern>/CoachFitness</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DependServlet</servlet-name>
    <url-pattern>/Depend</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RelyServlet</servlet-name>
    <url-pattern>/Rely</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CourseServlet</servlet-name>
    <url-pattern>/Course</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DailyCheckServlet</servlet-name>
    <url-pattern>/DailyCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CommentServet</servlet-name>
    <url-pattern>/Comment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FavorServlet</servlet-name>
    <url-pattern>/Favor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewsServlet</servlet-name>
    <url-pattern>/News</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadServlet</servlet-name>
    <url-pattern>/Download</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrainServlet</servlet-name>
    <url-pattern>/Train</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <error-page>
  	<error-code>404</error-code>
  	<location>/404.html</location>
  </error-page>
  
  <error-page>
  	<error-code>503</error-code>
  	<location>/503.html</location>
  </error-page>
  
  
</web-app>